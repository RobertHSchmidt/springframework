<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:u="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<bean class="org.springframework.config.java.process.ConfigurationPostProcessor" >
		<property name="namingStrategy">
			<bean class="org.springframework.config.java.naming.PropagateBeanNamingTest$TestNamingStrategy" />
		</property>
	</bean>

	<bean class="org.springframework.config.java.naming.PropagateBeanNamingTest$TestConfiguration" />

</beans>