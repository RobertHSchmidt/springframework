<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:u="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<bean class="org.springframework.config.java.process.NewConfigurationPostProcessor" />

	<bean class="org.springframework.config.java.JavaAndXmlTests$TestConfiguration" />

	<!-- This list is only there to make sure bean "root" gets instantiated before bean "dependency" -->
	<u:list id="all-beans">
		<ref bean="root" />
		<ref bean="dependency" />
	</u:list>

	<u:list id="root-dependencies">
		<ref bean="dependency" />
	</u:list>

</beans>